<div class="container">
  <div class="row">
    {{#each numberOfImages}}
       <div class="col-md-6">
          <div class="thumbnail">
          <img src="/images/{{../city}}/{{../city}}-{{this}}.jpg"/>
          </div>
       </div>
      {{else}}
        <h1>No images to show</h1>
    {{/each}}
  </div>
</div>
